{
    "name": "trinavo/laravel-ownable",
    "description": "A Laravel package for managing ownership of models.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Feras Abdalrahman",
            "email": "doon@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "spatie/laravel-permission": "^6.16"
    },
    "autoload": {
        "psr-4": {
            "Trinavo\\Ownable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trinavo\\Ownable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trinavo\\Ownable\\Providers\\OwnableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/framework": "^12.0",
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^10.0"
    }
}
